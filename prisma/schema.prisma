// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Car {
  id       Int    @id @default(autoincrement())
  make     String
  model    String
  year     Int
  hp       Int
  city_mpg Float
  hwy_mpg  Float
}
